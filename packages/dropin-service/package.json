{
    "name": "dropin-service",
    "private": true,
    "version": "0.0.1",
    "type": "module",
    "scripts": {
        "dev": "vite dev",
        "build": "vite build",
        "preview": "npm run build && wrangler dev",
        "format-code": "prettier --write . && eslint . --fix",
        "test:unit": "vitest",
        "test:storybook": "test-storybook -i",
        "test": "npm run test:unit -- --run && npm run test:storybook",
        "test:static": "concurrently -k -s first -n \"SB,TEST\" -c \"magenta,blue\" \"npm run build-storybook --quiet && npx http-server storybook-static --port 6006 --silent\" \"wait-on tcp:6006 && npm run test:storybook\"",
        "storybook": "storybook dev -p 6006",
        "build-storybook": "storybook build",
        "deploy-dev": "npm run build && wrangler deploy --env dev",
        "deploy-qa": "npm run build && wrangler deploy --env qa",
        "deploy-uat": "npm run build && wrangler deploy --env uat",
        "deploy-prod": "npm run build && wrangler deploy --env prod"
    },
    "devDependencies": {
        "@eslint/compat": "^1.2.5",
        "@eslint/js": "^9.18.0",
        "@storybook/addon-essentials": "^8.5.1",
        "@storybook/addon-interactions": "^8.5.1",
        "@storybook/addon-svelte-csf": "^5.0.0-next.23",
        "@storybook/blocks": "^8.5.1",
        "@storybook/svelte": "^8.5.1",
        "@storybook/sveltekit": "^8.5.1",
        "@storybook/test": "^8.5.1",
        "@storybook/test-runner": "^0.21.0",
        "@storybook/testing-library": "^0.2.1",
        "@sveltejs/adapter-cloudflare": "^5.0.1",
        "@sveltejs/kit": "^2.0.0",
        "@sveltejs/vite-plugin-svelte": "^4.0.0",
        "@tailwindcss/container-queries": "^0.1.1",
        "@tailwindcss/typography": "^0.5.16",
        "autoprefixer": "^10.4.20",
        "eslint": "^9.18.0",
        "eslint-config-prettier": "^10.0.1",
        "eslint-plugin-svelte": "^2.46.1",
        "globals": "^15.14.0",
        "msw": "^2.7.0",
        "prettier": "^3.4.2",
        "prettier-plugin-svelte": "^3.3.3",
        "prettier-plugin-tailwindcss": "^0.6.10",
        "sass": "^1.83.4",
        "sass-embedded": "^1.83.4",
        "storybook": "^8.5.1",
        "svelte": "^5.0.0",
        "tailwindcss": "^3.4.17",
        "vite": "^5.4.11",
        "vitest": "^2.1.8",
        "wrangler": "^3.105.0"
    },
    "dependencies": {
        "@inlang/paraglide-sveltekit": "^0.15.5",
        "@storybook/jest": "^0.2.2"
    },
    "msw": {
        "workerDirectory": [
            ".storybook/static"
        ]
    }
}
