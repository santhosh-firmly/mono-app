{
	"name": "dropin-service",
	"private": true,
	"version": "0.2.2",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "npm run prepare && npm run build-sdk && vite build",
		"preview": "vite preview",
		"preview:cloudflare": "wrangler dev --env dev",
		"prepare": "svelte-kit sync || echo ''",
		"format": "prettier --write .",
		"format-code": "prettier --write . && eslint . --fix",
		"format-code:check": "prettier --check . && eslint . --fix=false",
		"lint": "prettier --check . && eslint .",
		"test:unit": "vitest",
		"test": "npm run test:unit -- --run",
		"storybook": "storybook dev -p 6006",
		"build-storybook": "storybook build",
		"build-sdk": "node tools/build-sdk.mjs",
		"deploy-dev": "npm run build && wrangler deploy --env dev",
		"deploy-ci": "npm run build && wrangler deploy --env ci",
		"deploy-qa": "npm run build && wrangler deploy --env qa",
		"deploy-uat": "npm run build && wrangler deploy --env uat",
		"deploy-prod": "npm run build && wrangler deploy --env prod"
	},
	"devDependencies": {
		"@chromatic-com/storybook": "^3.2.5",
		"@cloudflare/vite-plugin": "^0.1.17",
		"@eslint/compat": "^1.2.5",
		"@eslint/js": "^9.18.0",
		"@iconify/svelte": "^4.2.0",
		"@storybook/addon-essentials": "^8.6.4",
		"@storybook/addon-interactions": "^8.6.4",
		"@storybook/addon-postcss": "^2.0.0",
		"@storybook/addon-svelte-csf": "^5.0.0-next.27",
		"@storybook/blocks": "^8.6.4",
		"@storybook/svelte": "^8.6.4",
		"@storybook/sveltekit": "^8.6.4",
		"@storybook/test": "^8.6.4",
		"@sveltejs/adapter-cloudflare": "^6.0.1",
		"@sveltejs/kit": "^2.16.0",
		"@sveltejs/vite-plugin-svelte": "^5.0.0",
		"@tailwindcss/vite": "^4.0.0",
		"@testing-library/jest-dom": "^6.6.3",
		"@testing-library/svelte": "^5.2.4",
		"eslint": "^9.18.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-svelte": "^3.0.0",
		"foundation": "github:goyalu/foundation#v1.4.1",
		"globals": "^16.0.0",
		"jsdom": "^26.0.0",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"prettier-plugin-tailwindcss": "^0.6.11",
		"storybook": "^8.6.4",
		"svelte": "^5.0.0",
		"tailwindcss": "^4.0.0",
		"vite": "^6.0.0",
		"vitest": "^3.0.0",
		"wrangler": "^4.9.1"
	},
	"dependencies": {
		"@inlang/paraglide-sveltekit": "^0.15.5",
		"@lottiefiles/svelte-lottie-player": "^0.3.1",
		"@storybook/jest": "^0.2.3",
		"@storybook/testing-library": "^0.2.2",
		"classnames": "^2.5.1",
		"colord": "^2.9.3",
		"flowbite": "^3.1.2",
		"flowbite-svelte": "^0.48.4",
		"msw": "^2.7.3",
		"sass-embedded": "^1.86.1",
		"superstruct": "^2.0.2",
		"svelte-forms-lib": "^2.0.1",
		"vaul-svelte": "^0.3.2",
		"yup": "^1.6.1"
	}
}
