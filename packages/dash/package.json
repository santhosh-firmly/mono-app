{
  "name": "new-dash",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write .",
    "build-dev": "vite build --mode dev",
    "build-uat": "vite build --mode uat",
    "build-prod": "vite build --mode prod",
    "dev-binding": "npm run build && wrangler pages dev .svelte-kit/cloudflare --compatibility-date=2024-02-19",
    "deploy-dev": "npm run build-dev && mkdir -p node_modules/.cache/wrangler/ && cp pages-uat.json node_modules/.cache/wrangler/pages.json && cp wrangler-dev.toml wrangler.toml wrangler pages deploy --branch main",
    "deploy-uat": "npm run build-uat && mkdir -p node_modules/.cache/wrangler/ && cp pages-uat.json node_modules/.cache/wrangler/pages.json && cp wrangler-uat.toml wrangler.toml && wrangler pages deploy --branch main",
    "deploy-prod": "npm run build-prod && mkdir -p node_modules/.cache/wrangler/ && cp pages-prod.json node_modules/.cache/wrangler/pages.json && cp wrangler-prod.toml wrangler.toml && wrangler pages deploy --branch main",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^8.4.5",
    "@storybook/addon-interactions": "^8.4.5",
    "@storybook/addon-svelte-csf": "^4.1.7",
    "@storybook/blocks": "^8.4.5",
    "@storybook/svelte": "^8.4.5",
    "@storybook/sveltekit": "^8.4.5",
    "@storybook/test": "^8.4.5",
    "@sveltejs/adapter-auto": "^3.0.0",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "@tailwindcss/typography": "^0.5.14",
    "@types/eslint": "^9.6.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-storybook": "^0.11.1",
    "eslint-plugin-svelte": "^2.36.0",
    "globals": "^15.0.0",
    "monaco-editor": "^0.52.0",
    "prettier": "^3.1.1",
    "prettier-plugin-svelte": "^3.1.2",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "storybook": "^8.4.5",
    "svelte": "^4.2.7",
    "tailwindcss": "^3.4.9",
    "vite": "^5.0.3"
  },
  "type": "module",
  "dependencies": {
    "@sveltejs/adapter-cloudflare": "^4.7.2",
    "bits-ui": "^0.21.15",
    "clsx": "^2.1.1",
    "cmdk-sv": "^0.0.18",
    "date-fns": "^4.1.0",
    "jose": "^5.9.2",
    "lucide-svelte": "^0.441.0",
    "paneforge": "^0.0.6",
    "svelte-headless-table": "^0.18.2",
    "svelte-radix": "^1.1.1",
    "tailwind-merge": "^2.5.2",
    "tailwind-variants": "^0.2.1",
    "tailwindcss-animate": "^1.0.7",
    "vaul-svelte": "^0.3.2"
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  }
}
