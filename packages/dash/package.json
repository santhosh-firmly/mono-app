{
  "name": "dash",
  "private": true,
  "version": "0.37.0",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "npm run prepare && vite build",
    "preview": "vite preview",
    "preview:cloudflare": "npm run build && wrangler dev --env dev --remote",
    "prepare": "svelte-kit sync || echo ''",
    "format-code": "eslint . --fix",
    "format-code:check": "eslint . --fix=false",
    "dev-binding": "npm run build && wrangler pages dev .svelte-kit/cloudflare --compatibility-date=2024-02-19",
    "deploy-dev": "npm run build && wrangler deploy --env dev",
    "deploy-ci": "npm run build && wrangler deploy --env ci",
    "deploy-qa": "npm run build && wrangler deploy --env qa",
    "deploy-uat": "npm run build && wrangler deploy --env uat",
    "deploy-prod": "npm run build && wrangler deploy --env prod",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test:storybook": "test-storybook --index-json",
    "test:storybook:ci": "test-storybook --ci --index-json",
    "test:storybook:coverage": "test-storybook --coverage",
    "test:storybook:watch": "test-storybook --watch",
    "test:storybook:run": "concurrently -k -s first -n \"SB,TEST\" -c \"magenta,blue\" \"npx http-server storybook-static --port 6006 --silent\" \"npx wait-on tcp:6006 && npm run test:storybook:ci\"",
    "test": "npm run build-storybook && npm run test:storybook:run"
  },
  "devDependencies": {
    "@firmly/eslint-config": "^0.37.0",
    "@storybook/addon-docs": "^8.0.0",
    "@storybook/addon-svelte-csf": "^5.0.10",
    "@storybook/addon-themes": "^8.6.15",
    "@storybook/svelte": "^8.0.0",
    "@storybook/sveltekit": "^8.0.0",
    "@storybook/test-runner": "^0.19.0",
    "@sveltejs/adapter-cloudflare": "^7.0.1",
    "@sveltejs/kit": "^2.16.0",
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "cmdk-sv": "^0.0.19",
    "concurrently": "^9.2.1",
    "eslint": "*",
    "http-server": "^14.1.1",
    "lucide-svelte": "^0.562.0",
    "prettier": "*",
    "prettier-plugin-svelte": "^3.4.0",
    "remark-gfm": "^4.0.1",
    "storybook": "^8.0.0",
    "svelte": "^5.27.0",
    "tailwindcss": "^3.4.9",
    "vite": "^7.3.0",
    "wait-on": "^9.0.3",
    "wrangler": "*"
  },
  "dependencies": {
    "@cloudflare/vite-plugin": "^1.17.1",
    "foundation": "github:goyalu/foundation#*",
    "autoprefixer": "^10.4.21",
    "bits-ui": "^2.15.4",
    "chart.js": "^4.5.1",
    "clsx": "^2.1.1",
    "cropperjs": "^2.1.0",
    "date-fns": "^4.1.0",
    "jose": "^6.1.3",
    "paneforge": "^0.0.6",
    "svelte-headless-table": "^0.18.2",
    "svelte-radix": "^3.0.2",
    "tailwind-merge": "^2.5.2",
    "tailwind-variants": "^0.2.1"
  },
  "prettier": "./.prettierrc.js"
}
