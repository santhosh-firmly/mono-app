{
	"name": "dash",
	"private": true,
	"version": "0.5.0",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"preview:cloudflare": "npm run build && wrangler dev --env dev --remote",
		"prepare": "svelte-kit sync || echo ''",
		"format-code": "eslint . --fix",
		"format-code:check": "eslint . --fix=false",
		"build-dev": "vite build --mode dev",
		"build-uat": "vite build --mode uat",
		"build-prod": "vite build --mode prod",
		"dev-binding": "npm run build && wrangler pages dev .svelte-kit/cloudflare --compatibility-date=2024-02-19",
		"deploy-dev": "npm run build && wrangler deploy -e dev",
		"deploy-uat": "npm run build && wrangler deploy -e uat",
		"deploy-prod": "npm run build && wrangler deploy -e prod",
		"storybook": "storybook dev -p 6006",
		"build-storybook": "storybook build"
	},
	"devDependencies": {
		"@firmly/eslint-config": "^0.5.0",
		"@storybook/addon-essentials": "^8.4.5",
		"@storybook/addon-interactions": "^8.4.5",
		"@storybook/addon-svelte-csf": "^5.0.0-next.28",
		"@storybook/svelte": "^8.4.5",
		"@storybook/sveltekit": "^8.4.5",
		"@sveltejs/adapter-auto": "^4.0.0",
		"@sveltejs/adapter-cloudflare": "^7.0.1",
		"@sveltejs/kit": "^2.16.0",
		"@sveltejs/vite-plugin-svelte": "^5.0.0",
		"cmdk-sv": "^0.0.19",
		"eslint": "*",
		"lucide-svelte": "^0.503.0",
		"monaco-editor": "^0.52.0",
		"prettier": "*",
		"storybook": "^8.4.5",
		"svelte": "^5.27.0",
		"tailwindcss": "^3.4.9",
		"vite": "^6.2.5",
		"wrangler": "*"
	},
	"dependencies": {
		"autoprefixer": "^10.4.21",
		"clsx": "^2.1.1",
		"date-fns": "^4.1.0",
		"jose": "^5.9.2",
		"paneforge": "^0.0.6",
		"svelte-headless-table": "^0.18.2",
		"svelte-radix": "^1.1.1",
		"tailwind-merge": "^2.5.2",
		"tailwind-variants": "^0.2.1"
	},
	"prettier": "@firmly/eslint-config/prettier"
}
